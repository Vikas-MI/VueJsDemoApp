v>\n        </div>\n    </div>\n</template>\n<script>\nimport {toast} from \"vue3-toastify\";\n\nexport default {\n    name:\"Home\",\n    mounted() {\n        let user = localStorage.getItem('userInfo');\n        if(!user) {\n            this.$router.push({name: 'Login'})\n        }\n    },\n    data() {\n        return {\n            showErrorAndSuccessButtons: true,\n            hasError: false,\n            isLoading: false,\n            isSuccessfull: false\n        };\n    },\n    methods : {\n        Logout(){\n            this.showLoading()\n            localStorage.removeItem(\"userInfo\");\n            toast.success('Logout successfully.');\n            let user = localStorage.getItem('userInfo');\n            if(!user) {\n                setTimeout(function(){\n                    this.isLoading = false\n                    this.showErrorAndSuccessButtons = false\n                    this.$router.push({name: 'Home'})\n                },2000)\n            }\n            this.$router.push({name: 'Login'})\n        },\n        showLoading: function () {\n            this.hasError = !this.hasError;\n            this.isLoading = !this.isLoading;\n            this.showErrorAndSuccessButtons = !this.showErrorAndSuccessButtons;\n            console.log(\"Loading should be shown\");\n        },\n    }\n}\n</script>\n<style>\n.flex-styling {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    align-content: center;\n    height: 100vh;\n    background-color:  #34a0a4;\n    padding-left: 10%;\n    padding-right: 10%;\n}\n\n.white-font {\n    color: white;\n}\n\n.font-size-small {\n    font-size: 14px\n}\n\n.spacing {\n    margin: 10px\n}\n.button-styling {\n    border: none;\n    background-color: #e0fbfc;\n    color: black;\n    padding: 10px 18px;\n    border-radius: 4px;\n    margin-top: 10px;\n    margin-bottom: 20px;\n    font-family: helvetica;\n    box-shadow: 0 4px 8px 0 rgba(0, 0, 0.1, 0.1);\n}\n\n.bu
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { toast } from \"vue3-toastify\";\nimport Header from \"@/components/Header.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"Home\",\n  components: {\n    Header\n  },\n  computed: {\n    pages() {\n      let pages = [];\n      for (let i = 1; i <= Math.ceil(this.iteminfos.length / this.itemsPerPage); i++) {\n        pages.push(i);\n      }\n      return pages;\n    },\n    displayedItems() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.iteminfos.slice(start, end);\n    }\n  },\n  data() {\n    return {\n      name: '',\n      restaurant: [],\n      iteminfos: [{\n        id: 1,\n        name: 'Item 1'\n      }, {\n        id: 2,\n        name: 'Item 2'\n      }, {\n        id: 3,\n        name: 'Item 3'\n      }, {\n        id: 4,\n        name: 'Item 4'\n      }, {\n        id: 5,\n        name: 'Item 5'\n      }, {\n        id: 6,\n        name: 'Item 6'\n      }, {\n        id: 7,\n        name: 'Item 7'\n      }, {\n        id: 8,\n        name: 'Item 8'\n      }, {\n        id: 9,\n        name: 'Item 9'\n      }, {\n        id: 10,\n        name: 'Item 10'\n      }, {\n        id: 11,\n        name: 'Item 11'\n      }, {\n        id: 12,\n        name: 'Item 12'\n      }, {\n        id: 13,\n        name: 'Item 13'\n      }, {\n        id: 14,\n        name: 'Item 14'\n      }, {\n        id: 15,\n        name: 'Item 15'\n      }, {\n        id: 16,\n        name: 'Item 16'\n      }, {\n        id: 17,\n        name: 'Item 17'\n      }, {\n        id: 18,\n        name: 'Item 18'\n      }, {\n        id: 19,\n        name: 'Item 19'\n      }, {\n        id: 20,\n        name: 'Item 20'\n      }],\n      currentPage: 1,\n      itemsPerPage: 5\n    };\n  },\n  mounted() {\n    this.loadData();\n  },\n  methods: {\n    async deleteRestaurant(id) {\n      let result = await axios.delete(\"http://192.168.1.250:3000/restaurant/\" + id);\n      if (result.status === 200) {\n        toast.success('Record deleted successfully.');\n        await this.$router.push({\n          name: 'Home'\n        });\n        this.loadData();\n      }\n    },\n    async loadData() {\n      let user = localStorage.getItem('userInfo');\n      this.name = JSON.parse(user).name;\n      if (!user) {\n        this.$router.push({\n          name: 'Login'\n        });\n      }\n      let result = await axios.get(\"http://192.168.1.250:3000/restaurant\");\n      this.restaurant = result.data;\n    },\n    onClickHandler(page) {\n      console.log(page);\n    }\n  }\n};","map":{"version":3,"names":["toast","Header","axios","name","components","computed","pages","i","Math","ceil","iteminfos","length","itemsPerPage","push","displayedItems","start","currentPage","end","slice","data","restaurant","id","mounted","loadData","methods","deleteRestaurant","result","delete","status","success","$router","user","localStorage","getItem","JSON","parse","get","onClickHandler","page","console","log"],"sources":["/Applications/MAMP/htdocs/Vue/Demo/restaurant-app/restaurent-app/src/components/Home.vue"],"sourcesContent":["<template>\n    <Header></Header>\n    <h1 class=\"mt-10\">Hello {{ name }} Welcome to the dashboard.......</h1>\n    <div class=\"container\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-4 col-xl-3\">\n                <div class=\"card bg-c-blue order-card\">\n                    <div class=\"card-block\">\n                        <h6 class=\"m-b-20\">Total restaurant</h6>\n                        <h2 class=\"text-right\"><i class=\"fa fa-cart-plus f-left\"></i><span>{{ restaurant.length }}</span></h2>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <table class=\"table table-striped\">\n        <thead>\n            <tr>\n                <th scope=\"col\">#</th>\n                <th scope=\"col\">Name</th>\n                <th scope=\"col\">Contact</th>\n                <th scope=\"col\">Address</th>\n                <th scope=\"col\">Action</th>\n            </tr>\n        </thead>\n            <tbody v-if=\"restaurant.length > 0 \">\n                <tr v-for=\"item in restaurant\" :key=item.id>\n                        <th>{{item.id}}</th>\n                        <th>{{item.name}}</th>\n                        <th>{{item.contact}}</th>\n                        <th>{{item.address}}</th>\n                        <th><router-link :to=\"'/update-restaurant/'+item.id\"> <font-awesome-icon icon=\"pen\" />\n                        </router-link>  |  <a @click=\"deleteRestaurant(item.id)\" href=\"#\"><font-awesome-icon icon=\"trash\" /></a> </th>\n                </tr>\n            </tbody>\n            <tbody v-else>\n                <tr class=\"no-records-found\">\n                    <td colspan=\"5\">No matching records found</td>\n                </tr>\n            </tbody>\n\n\n    </table>\n\n    <ul>\n        <li v-for=\"page in pages\" :key=\"page\" @click=\"currentPage = page\" :class=\"{ active: currentPage === page }\">\n            {{ page }}\n        </li>\n    </ul>\n\n    <ul>\n        <li v-for=\"item in displayedItems\" :key=\"item.id\">\n            {{ item.name }}\n        </li>\n    </ul>\n</template>\n<script>\n\nimport {toast} from \"vue3-toastify\";\nimport Header from \"@/components/Header.vue\";\nimport axios from \"axios\";\nexport default {\n    name:\"Home\",\n    components : {\n        Header\n    },\n    computed:{\n        pages() {\n            let pages = [];\n            for (let i = 1; i <= Math.ceil(this.iteminfos.length / this.itemsPerPage); i++) {\n                pages.push(i);\n            }\n            return pages;\n        },\n        displayedItems() {\n            const start = (this.currentPage - 1) * this.itemsPerPage;\n            const end = start + this.itemsPerPage;\n            return this.iteminfos.slice(start, end);\n        }\n    },\n    data(){\n        return{\n            name :'',\n            restaurant : [],\n            iteminfos: [\n                { id: 1, name: 'Item 1' },\n                { id: 2, name: 'Item 2' },\n                { id: 3, name: 'Item 3' },\n                { id: 4, name: 'Item 4' },\n                { id: 5, name: 'Item 5' },\n                { id: 6, name: 'Item 6' },\n                { id: 7, name: 'Item 7' },\n                { id: 8, name: 'Item 8' },\n                { id: 9, name: 'Item 9' },\n                { id: 10, name: 'Item 10' },\n                { id: 11, name: 'Item 11' },\n                { id: 12, name: 'Item 12' },\n                { id: 13, name: 'Item 13' },\n                { id: 14, name: 'Item 14' },\n                { id: 15, name: 'Item 15' },\n                { id: 16, name: 'Item 16' },\n                { id: 17, name: 'Item 17' },\n                { id: 18, name: 'Item 18' },\n                { id: 19, name: 'Item 19' },\n                { id: 20, name: 'Item 20' }\n            ],\n            currentPage: 1,\n            itemsPerPage: 5\n        }\n    },\n    mounted() {\n        this.loadData()\n    },\n    methods:{\n        async deleteRestaurant(id){\n            let result = await axios.delete(\"http://192.168.1.250:3000/restaurant/\"+id)\n            if(result.status === 200){\n                toast.success('Record deleted successfully.');\n                await this.$router.push({name: 'Home'})\n                this.loadData()\n            }\n        },\n        async loadData(){\n            let user = localStorage.getItem('userInfo');\n            this.name = JSON.parse(user).name;\n            if(!user){\n                this.$router.push({name: 'Login'})\n            }\n            let result = await axios.get(\"http://192.168.1.250:3000/restaurant\");\n            this.restaurant = result.data\n        },\n        onClickHandler(page)  {\n            console.log(page);\n        }\